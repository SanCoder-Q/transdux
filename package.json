    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "ecstatic": "^1.3.1",
    "uglify-js": "^2.6.1",
    "watchify": "^3.6.1"
  },
  "author": "Jichao Ouyang",
  "license": "ISC",
  "babel": {
